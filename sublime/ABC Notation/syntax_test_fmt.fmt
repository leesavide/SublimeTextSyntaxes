	/Times-New-Roman-Bold-utf8 11 selectfont () dup
	show
	gsave
	0 0 RM  .7 SLW
	stringwidth exch neg 0 add exch RL stroke
	grestore
 	M -3 -6 RM
	-5 0 RM  .7 SLW
2 -3 RL
	16 add 0 RL 2 -3 RL
